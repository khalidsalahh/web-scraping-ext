{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "requireConfigFile": false,
    "sourceType": "module"
  },
  "plugins": ["@babel"],
  "rules": {
    "no-console": "warn"
  },
  "parser": "@babel/eslint-parser"
}
